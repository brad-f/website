{
    "extends": "airbnb",
    "plugins": [
      "jest"
    ],
    "rules": {
      "comma-dangle": "off",
      "prefer-destructuring": "off",
      "react/jsx-filename-extension": "off",
      "react/prop-types": "off",
      "jest/no-disabled-tests": "warn",
      "jest/no-focused-tests": "error",
      "jest/no-identical-title": "error",
      "jest/valid-expect": "error"
    },
    "env": {
      "jest/globals": true
    }
}
